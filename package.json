{
  "name": "vitepress-theme-ansidev",
  "version": "1.0.0",
  "description": "The VitePress theme for ansidev's blog.",
  "main": "./dist/client/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./composables": {
      "types": "./dist/server/composables/index.d.ts",
      "default": "./dist/server/composables/index.js"
    },
    "./config": {
      "types": "./dist/client/vitepress/config.d.ts",
      "default": "./dist/server/config/baseConfig.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansidev/vitepress-theme-ansidev.git"
  },
  "keywords": [
    "vitepress-theme-ansidev",
    "vitepress-theme",
    "vitepress",
    "theme",
    "ansidev"
  ],
  "author": "Le Minh Tri &lt;ansidev@gmail.com&gt;",
  "license": "MIT",
  "homepage": "https://github.com/ansidev/vitepress-theme-ansidev",
  "bugs": {
    "url": "https://github.com/ansidev/vitepress-theme-ansidev/issues"
  },
  "scripts": {
    "lint": "prettier --check --write --parser typescript \"{__tests__,docs,scripts,src,types}/**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"{__tests__,docs,src,types}/**/*.ts\"",
    "type": "vue-tsc --noEmit",
    "test": "npm run lint && npm run type",
    "dev": "vitepress dev demo",
    "build": "node scripts/build.mjs",
    "serve": "pnpm demo:build && vitepress serve demo",
    "clean": "rimraf dist",
    "theme:build": "vite build --config vite.theme.config.ts",
    "server:build": "tsdown src/server/index.ts src/core/index.ts src/client/vitepress/config.ts --format esm --out-dir dist --dts",
    "demo:install-local": "mkdir -p .tmp && pnpm pack --pack-destination .tmp && pnpm --dir demo add \"../$(ls -t .tmp/*.tgz | head -n 1)\" --save-exact --offline --config.auto-install-peers=false",
    "demo:build": "pnpm build && pnpm demo:install-local && NODE_ENV=production vitepress build demo"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^22.13.0",
    "autoprefixer": "^10.4.21",
    "glob": "^11.0.1",
    "gray-matter": "^4.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^7.1.4",
    "vitepress": "^2.0.0-alpha.12",
    "vue": "^3.5.13",
    "vue-tsc": "^3.0.0"
  },
  "peerDependencies": {
    "vitepress": "^2.0.0-alpha.12",
    "@vueuse/core": "^13.0.0",
    "vue-i18n": "^11.1.0",
    "vue": "^3.5.13"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "esbuild"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "packageManager": "pnpm@10.15.1",
  "types": "./dist/core/index.d.ts"
}
