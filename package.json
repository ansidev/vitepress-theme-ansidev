{
  "name": "vitepress-theme-ansidev",
  "version": "1.0.4",
  "private": true,
  "description": "The VitePress theme for ansidev's blog",
  "type": "module",
  "packageManager": "pnpm@10.30.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "typecheck": "pnpm -r typecheck",
    "lint:check": "pnpm biome check",
    "lint": "pnpm -r typecheck && pnpm lint:check --write",
    "demo:dev": "pnpm --filter @ansidev/vitepress-theme-ansidev-demo dev",
    "demo:build": "pnpm --filter @ansidev/vitepress-theme-ansidev-demo build",
    "demo:preview": "pnpm --filter @ansidev/vitepress-theme-ansidev-demo preview"
  },
  "keywords": [
    "vitepress",
    "theme",
    "blog",
    "tailwindcss",
    "vue3",
    "typescript",
    "i18n",
    "dark-mode",
    "ansidev"
  ],
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.4.2"
  }
}
