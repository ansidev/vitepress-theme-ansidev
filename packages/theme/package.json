{
  "name": "@ansidev/vitepress-theme-ansidev",
  "version": "1.0.2",
  "description": "The VitePress theme for ansidev's blog",
  "type": "module",
  "main": "dist/node/index.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "author": "Le Minh Tri <ansidev@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansidev/vitepress-theme-ansidev.git"
  },
  "keywords": [
    "vitepress",
    "theme",
    "blog",
    "tailwindcss",
    "vue3",
    "typescript",
    "i18n",
    "dark-mode",
    "ansidev"
  ],
  "exports": {
    ".": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./styles.css": "./dist/client/styles.css",
    "./config": {
      "import": "./dist/node/config.mjs",
      "types": "./dist/node/config.d.ts"
    },
    "./composables": {
      "import": "./dist/node/composables.mjs",
      "types": "./dist/node/composables.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:client": "vite build && tsc --emitDeclarationOnly",
    "build:node": "tsdown",
    "build": "pnpm clean && pnpm build:client && pnpm build:node",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@vueuse/core": "^14.2.1",
    "vue-i18n": "^11.2.8"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "autoprefixer": "^10.4.24",
    "glob": "^13.0.5",
    "gray-matter": "^4.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "rollup-plugin-copy": "^3.5.0",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitepress": "^2.0.0-alpha.16",
    "vue": "^3.5.28",
    "vue-tsc": "^3.2.4"
  },
  "peerDependencies": {
    "vitepress": "^2.0.0-alpha.16",
    "vue": "^3.5.28"
  },
  "packageManager": "pnpm@10.30.0"
}
